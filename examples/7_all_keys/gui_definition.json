{
    "gui": [
        {
            "label": "Components Minimal",
            "command": "echo file: %% & echo folder: %% & echo choice: %% & echo check: %% & echo check_array: %% & echo textbox: %%",
            "components": [
                {
                    "type": "static_text",
                    "label": "All components with their required keys."
                },
                {
                    "type": "file",
                    "label": "Some file path"
                },
                {
                    "type": "folder",
                    "label": "Some folder path"
                },
                {
                    "type": "choice",
                    "label": "combo box",
                    "items": ["value1", "value2", "value3"]
                },
                {
                    "type": "check",
                    "label": "flag!"
                },
                {
                    "type": "check_array",
                    "label": "options",
                    "items": ["falg1", "falg2", "falg3", "flag4"]
                },
                {
                    "type": "text_box",
                    "label": "text box"
                }
            ]
        },
        {
            "label": "Components Optional",
            "command": "echo file: %file% & echo folder: %folder% & echo choice: %choice% & echo check: %check% & echo check_array: %options% & echo textbox: %text%",
            "components": [
                {
                    "type": "static_text",
                    "label": "All components with their optinal keys."
                },
                {
                    "type": "file",
                    "label": "PNG or JPG",
                    "id": "file",
                    "extension": "png or jpg (*.png;*.jpg)|*.png;*.jpg",
                    "empty_message": "Drop a file here!",
                    "default": "test.txt",
                    "add_quotes": true
                },
                {
                    "type": "folder",
                    "label": "Folder path",
                    "id": "folder",
                    "empty_message": "Drop a folder here!",
                    "default": "testdir",
                    "add_quotes": true
                },
                {
                    "type": "check",
                    "label": "Checkbox",
                    "id": "check",
                    "value": "flag!",
                    "default": true
                },
                {
                    "type": "choice",
                    "label": "Combo box",
                    "id": "choice",
                    "items": ["item1", "item2", "item3"],
                    "values": ["value1", "value2", "value3"],
                    "default": 2
                },
                {
                    "type": "check_array",
                    "label": "Options",
                    "id": "options",
                    "items": ["item1", "item2", "item3", "item4"],
                    "values": ["falg1", "falg2", "falg3", "flag4"],
                    "default": [false, false, true, false]
                },
                {
                    "type": "text_box",
                    "label": "Text box",
                    "id": "text",
                    "empty_message": "type here!",
                    "default": "remove this text!"
                }
            ]
        },
        {
            "label": "GUI Optional",
            "window_name": "Window Title Here",
            "command": "echo this won't be showed up & echo sample message!",
            "button": "Run!",
            "show_last_line": true,
            "components": [
                {
                    "type": "static_text",
                    "label": "show_last_line can show a message."
                }
            ]
        }
    ],
    "help": [
        {
            "type": "url",
            "label": "test",
            "url":"http://test.com"
        }
    ]
}