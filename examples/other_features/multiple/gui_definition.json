{
    "gui": [
        {
            "label": "Sample GUI",
            "command": "echo file: %-% & echo folder: %-% & echo checkbox: %-%",
            "button": "Echo!",
            "components": [
                {
                    "type": "static_text",
                    "label": "This is a sample GUI. Edit 'gui_definition.json' for your scripts."
                },
                {
                    "type": "file",
                    "label": "Some file path",
                    "extension": "any files (*)|*",
                    "empty_message": "Drop a file here!"
                },
                {
                    "type": "folder",
                    "label": "Some folder path"
                },
                {
                    "type": "check",
                    "label": "checkbox",
                    "value": "true",
                    "default": true
                }
            ]
        },
        {
            "label": "Sample GUI2",
            "command": "echo text_box: %-% & echo options:%-%",
            "button": "Echo!",
            "components": [
                {
                    "type": "static_text",
                    "label": "This is a sample GUI. Edit 'gui_definition.json' for your scripts."
                },
                {
                    "type": "text",
                    "label": "Some text"
                },
                {
                    "type": "check_array",
                    "label": "options",
                    "item": ["option1 ", "option2 ", " flag"],
                    "value": [" --option1 ", " --option2 ", " -flag"],
                    "default": [true, false, false]
                }
            ]
        }
    ]
}
