{
    "recommended": "0.3.0",
    "minimum_required": "0.3.0",
    "gui": [
        {
            "label": "Components Minimal",
            "command": "echo file: %-% & echo folder: %-% & echo choice: %-% & echo check: %-% & echo check_array: %-% & echo textbox: %-% & echo int: %-% & echo float: %-%",
            "components": [
                {
                    "type": "static_text",
                    "label": "All components with their required keys."
                },
                {
                    "type": "file",
                    "label": "Some file path"
                },
                {
                    "type": "folder",
                    "label": "Some folder path"
                },
                {
                    "type": "choice",
                    "label": "combo box",
                    "item": ["value1", "value2", "value3"]
                },
                {
                    "type": "check",
                    "label": "flag!"
                },
                {
                    "type": "check_array",
                    "label": "options",
                    "item": ["flag1", "flag2", "flag3", "flag4"]
                },
                {
                    "type": "text",
                    "label": "text box"
                },
                {
                    "type": "int",
                    "label": "Int picker"
                },
                {
                    "type": "float",
                    "label": "Float picker"
                }
            ]
        },
        {
            "label": "Components Optional",
            "command": "echo file: %file% & echo folder: %folder% & echo choice: %choice% & echo check: %check% & echo check_array: %options% & echo textbox: %text% & echo int: %integer% & echo float: %double%",
            "components": [
                {
                    "type": "static_text",
                    "label": "All components with their optinal keys."
                },
                {
                    "type": "file",
                    "label": "PNG or JPG",
                    "id": "file",
                    "extension": "png or jpg (*.png;*.jpg)|*.png;*.jpg",
                    "empty_message": "Drop a file here!",
                    "default": "test.txt",
                    "add_quotes": true,
                    "tooltip": "Tooltip!"
                },
                {
                    "type": "folder",
                    "label": "Folder path",
                    "id": "folder",
                    "empty_message": "Drop a folder here!",
                    "default": "testdir",
                    "add_quotes": true,
                    "tooltip": "Tooltip?"
                },
                {
                    "type": "check",
                    "label": "Checkbox",
                    "id": "check",
                    "value": "flag!",
                    "default": true,
                    "tooltip": "check this flag!"
                },
                {
                    "type": "choice",
                    "label": "Combo box",
                    "id": "choice",
                    "item": ["item1", "item2", "item3"],
                    "value": ["value1", "value2", "value3"],
                    "default": 2,
                    "tooltip": "choose one of items."
                },
                {
                    "type": "check_array",
                    "label": "Options",
                    "id": "options",
                    "item": ["item1", "item2", "item3", "item4"],
                    "value": [" --f1", " --f2", " --f3", " --f4"],
                    "default": [false, false, true, false],
                    "tooltip": ["flag1", "flag2", "flag3", "flag4"]
                },
                {
                    "type": "text",
                    "label": "Text box",
                    "id": "text",
                    "empty_message": "type here!",
                    "default": "remove this text!",
                    "tooltip": "text box."
                },
                {
                    "type": "int",
                    "label": "Int picker",
                    "id": "integer",
                    "default": 10,
                    "min": -50,
                    "max": 50,
                    "inc": 10,
                    "wrap": true
                },
                {
                    "type": "float",
                    "label": "Float picker",
                    "id": "double",
                    "default": 0.01,
                    "min": 0,
                    "max": 1,
                    "digits": 2,
                    "inc": 0.01,
                    "wrap": false
                }
            ]
        },
        {
            "label": "GUI Optional",
            "window_name": "Window Title Here",
            "command": "echo cwd: %__CWD__% & echo percent: %% & echo sample message!",
            "button": "Run!",
            "show_last_line": true,
            "components": [
                {
                    "type": "static_text",
                    "label": "show_last_line can show a message."
                }
            ]
        }
    ],
    "help": [
        {
            "type": "url",
            "label": "test",
            "url":"http://test.com"
        }
    ]
}
